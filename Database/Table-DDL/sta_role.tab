create table STA_ROLE
(
    ID           NUMBER generated by default on null as identity
        constraint STA_RLE_PK
            primary key,
    NAME         VARCHAR2(40) not null
        constraint STA_RLE_UK1
            unique,
    SYSTEM_NAME  VARCHAR2(40)
        constraint STA_ROLE_UK2
            unique,
    SYSTEM_NAME2 VARCHAR2(40)
)
/