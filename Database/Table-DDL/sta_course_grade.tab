create table sta_course_grade (
    id number generated by default on null as identity,
    crse_id number not null,
    grde_id number not null,
    constraint sta_crse_grde_pk primary key (id),
    constraint sta_crse_grde_crse_fk foreign key (crse_id) references sta_course (id),
    constraint sta_crse_grde_grde_fk foreign key (grde_id) references sta_grade (id),
    constraint sta_crse_grde_uk1 unique(crse_id, grde_id)
);


