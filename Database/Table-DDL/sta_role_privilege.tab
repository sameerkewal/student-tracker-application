create table sta_role_privilege (
    id number generated by default on null as identity,
    rle_id number  not null,
    pve_id number not null,
    constraint sta_rle_pve_pk primary key(id),
    constraint sta_rle_pve_rle_fk foreign key(rle_id) references sta_role,
    constraint sta_rle_pve_pve_fk foreign key(pve_id) references sta_privilege,
    constraint sta_rle_pve_uk1 unique(rle_id, pve_id)
);


drop table sta_rle_pve;