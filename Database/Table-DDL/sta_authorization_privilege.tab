create table sta_authorization_privilege(
    id number generated by default on null as identity,
    auth_id number not null,
    pve_id number not null,
    constraint sta_auth_pve_pk primary key(id),
    constraint sta_auth_pve_auth_fk foreign key(auth_id)references sta_authorization,
    constraint sta_auth_pve_pve_fk foreign key(pve_id) references sta_privilege
);