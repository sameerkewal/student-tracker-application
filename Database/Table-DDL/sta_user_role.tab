create table sta_user_role (
    id number generated by default on null as identity,
    usr_id number not null,
    rle_id number not null,
    constraint sta_usr_rle_pk primary key(id),
    constraint sta_usr_rle_usr_fk foreign key(usr_id) references sta_user,
    constraint sta_usr_rle_rle_fk foreign key(rle_id) references sta_role
);

drop table sta_user_role;



