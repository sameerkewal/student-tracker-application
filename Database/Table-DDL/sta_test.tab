create table sta_test (
    id number generated by default on null as identity,
    name varchar2(80) not null,
    smsr_id number not null,
    grde_id number not null,
    crse_id number not null,
    constraint sta_tst_pk primary key(id),
    constraint sta_tst_smsr_fk foreign key(smsr_id) references sta_semester,
    constraint sta_tst_grde_fk foreign key(grde_id) references sta_grade,
    constraint sta_tst_crse_fk foreign key(crse_id) references sta_course
);

create unique index sta_tst_ix1 on sta_test (lower("NAME"));
drop table sta_test;

